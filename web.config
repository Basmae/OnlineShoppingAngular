<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="rewrite url yo index.html">
                    <match url=".*\.html|css/|img/|js/|app/|assets/|node_modules/|lib/|favicon.ico" negate="true" />
                    <action type="Rewrite" url="/index.html" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
